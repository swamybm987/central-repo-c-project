pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                git branch: 'main', url: 'https://github.com/vinay542486/central-repo-c-project.git'
                    sh ''' 
                       cd /var/lib/jenkins/workspace/cprojassig
                       make ABC.exe
                    ''' 
                    }
                }
         stage('copying artifacts') {
            steps {
                    sshagent (credentials: ['6f6ff3af-3a02-4a96-a4d3-f102abf04d79']) {
                    sh 'ssh -o StrictHostKeyChecking no ec2-user@172.31.33.18 uptime'
                    sh 'ssh -v ec2-user@172.31.33.18'
                    }
            }
        }
    }
}
